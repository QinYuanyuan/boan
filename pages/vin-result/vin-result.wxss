page{
  width: 750rpx;
  height: 100%;
  background-color: #fff;
}
.vin-container{
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  width: 750rpx;
  padding: 30rpx 40rpx 20rpx 40rpx;
  border-bottom: 2rpx dashed #e5e5e5;
}
.vin-container view{
  margin-right: 10rpx;
  width: 30rpx;
  height: 54rpx;
  line-height: 54rpx;
  text-align: center;
  color: #f90a05;
  font-size: 30rpx;
  background-color: #d9d9d9;
}
.vin-container view:nth-last-child(1){
  margin-right: 0;
}

.text{
  width: 750rpx;
  line-height: 78rpx;
  text-align: center;
  color: #353535;
  font-size: 28rpx;
}

.vin-result .one-result{
  position: relative;
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
  width: 750rpx;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #f7f7f7;
  color: #353535;
  font-size: 28rpx;
}
.vin-result .one-result .number{
  margin-right: 20rpx;
}
.vin-result .one-result .arrow{
  position: absolute;
  top: 28rpx;
  right: 20rpx;
  width: 20rpx;
  height: 20rpx;
  border: 1rpx solid #666;
  border-left: 1rpx solid transparent;
  border-bottom: 1rpx solid transparent;
  transform: rotate(45deg);
}

.no-result{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 80rpx;
}
.no-result image{
  width: 256rpx;
  height: 240rpx;
}
.no-result .fail-text{
  color: #f90a05;
  font-size: 30rpx;
  line-height: 50rpx;
}
.no-result .tip{
  margin: 10rpx 50rpx 72rpx 50rpx;
  color: #5c5c5c;
  font-size: 28rpx;
  text-align: center;
}
.no-result .go-back-btn{
  width: 650rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 34rpx;
  background-color: #f90a05;
  border-radius: 10rpx;
  box-shadow: 5rpx 5rpx 18rpx rgba(117,177,117,.2);
}